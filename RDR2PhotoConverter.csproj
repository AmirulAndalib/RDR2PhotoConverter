<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows10.0.17763.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>True</UseWindowsForms>
    <SupportedOSPlatformVersion>10.0.17763.0</SupportedOSPlatformVersion>
    <RootNamespace>$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="background.jpg" />
    <None Remove="bluesky.png" />
    <None Remove="cross-icon.png" />
    <None Remove="github-logo.png" />
    <None Remove="github.png" />
    <None Remove="twitterx.png" />
    <None Remove="x-logo.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="background.jpg" />
    <Resource Include="bluesky.png" />
    <Resource Include="cross-icon.png" />
    <Resource Include="github.png" />
    <Resource Include="twitterx.png" />
  </ItemGroup>

</Project>
